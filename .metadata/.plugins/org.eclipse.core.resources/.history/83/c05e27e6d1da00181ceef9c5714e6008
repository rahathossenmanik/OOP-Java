package main;

import java.util.EnumSet;

public enum Apple {
	Jonathan(10), GoldenDel, RedDel(12), Winesap(15);
	private int price;
	Apple(int p){ price = p;}
	Apple(){ price = -1;}
	int getPrice(){ return price;}
	
	public static void main(String[] args){
        System.out.println("Hello");
        Apple.Jonathan.price=5;
        EnumSet.allOf(Apple.class) 
        .forEach(season -> System.out.println(season));
    }
}
